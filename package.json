{
  "name": "dlm",
  "productName": "DLM",
  "version": "1.0.0",
  "description": "Programmazione attivit√† Liceo Don Lorenzo Milani",
  "main": "main.js",
  "scripts": {
    "package-win": "electron-packager . --asar --overwrite --platform=win32 --arch=ia32 --icon=icons/win/icon.ico --prune=true --out=release-builds",
    "setup": "electron-installer-windows --config installer.json",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\""
  },
  "keywords": [],
  "author": {
    "name": "Alberto Finardi",
    "email": "finardialberto@gmail.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "dlm"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  },
  "dependencies": {
    "electron-builder": "^22.9.1",
    "electron-packager": "^15.2.0",
    "electron-squirrel-startup": "^1.0.0",
    "electron-store": "^6.0.1",
    "excel4node": "^1.7.2",
    "fix-path": "^3.0.0",
    "jquery": "^3.5.1",
    "uuidv4": "^6.2.6"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.53",
    "@electron-forge/maker-deb": "^6.0.0-beta.53",
    "@electron-forge/maker-rpm": "^6.0.0-beta.53",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.53",
    "@electron-forge/maker-zip": "^6.0.0-beta.53",
    "electron": "^10.1.2",
    "electron-icon-maker": "0.0.5",
    "electron-installer-windows": "^3.0.0",
    "electron-winstaller": "^4.0.1"
  }
}
